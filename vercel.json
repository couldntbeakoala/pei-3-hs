{
  "version": 2,
  "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
  "framework": "nodejs",
  "cleanUrls": true,
  "functions": {
    "api/index.js": {
      "memory": 3009,
      "schedule": ["0 * * * *"]
    }
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {}
    }
  ],
  "routes": [
    {
      "src": "api/*",
      "dest": "api/index.js",
    }
  ]
}
